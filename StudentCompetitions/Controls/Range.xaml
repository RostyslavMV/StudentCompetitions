<UserControl x:Class="StudentCompetitions.Range"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StudentCompetitions"
             mc:Ignorable="d" 
             >
    <StackPanel Orientation="Horizontal">
        <TextBlock x:Name="FromText" Text=" from "/>
        <TextBox x:Name="FromBox" Width="70" IsEnabled="False" TextChanged="FromBox_TextChanged"/>
        <TextBlock x:Name="ToText" Text=" to "/>
        <TextBox x:Name="ToBox" Width="70" IsEnabled="False" TextChanged="ToBox_TextChanged"/>
    </StackPanel>
</UserControl>
